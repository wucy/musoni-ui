<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.musoni"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.INTERNET" >
    </uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/myTheme" >
        <activity
            android:name="com.musoni.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.musoni.LogInActivity"
            android:label="@string/title_activity_log_in" >
        </activity>
        <activity
            android:name="com.musoni.BarInflator"
            android:label="@string/barInflator" >
        </activity>
        <activity
            android:name="com.musoni.ClientRegActivity"
            android:label="@string/title_activity_client_reg" >
        </activity>
        <activity
            android:name="com.musoni.LoanApplication"
            android:label="@string/title_activity_loan_application" >
        </activity>
        <activity
            android:name="com.musoni.Welcome"
            android:label="@string/title_activity_welcome" >
        </activity>

        <uses-library android:name="android.test.runner" />

        <activity
            android:name="com.musoni.QueueTaskActivity"
            android:label="@string/title_activity_queue_task" >
        </activity>
        
        <activity
            android:name="com.musoni.GroupRegSimp"
            android:label="@string/title_activity_group_reg_simp" >
        </activity>
    </application>

    <instrumentation
        android:name="android.test.InstrumentationTestRunner"
        android:label="AuthTest"
        android:targetPackage="com.musoni" />

</manifest>
